{
  "plugins": [
    "git-tag",
    "pr-body-labels",
    [
      "exec",
      {
        "beforeCommitChangelog": "[ $GITHUB_REF != '/ref/heads/next' ] || make all"
      }
    ]
  ],
  "owner": "mariadb-corporation",
  "repo": "skysql-api-go",
  "name": "SkySQL API Golang SDK Releases",
  "email": "no-reply@mariadb.com",
  "prereleaseBranches": ["next"],
  "noDefaultLabels": true,
  "labels": [
    {
      "name": "breaking",
      "changelogTitle": "ğŸ’¥ Breaking Change",
      "description": "Increment the major version when merged",
      "releaseType": "patch",
      "color": "#C5000B"
    },
    {
      "name": "enhancement",
      "changelogTitle": "ğŸš€ Enhancement",
      "description": "Increment the minor version when merged",
      "releaseType": "patch",
      "color": "#F1A60E"
    },
    {
      "name": "bugfix",
      "changelogTitle": "ğŸ› Bug Fix",
      "description": "Increment the patch version when merged",
      "releaseType": "patch",
      "color": "#870048"
    },
    {
      "name": "dependencies",
      "changelogTitle": "ğŸ”© Dependency Updates",
      "description": "Update one or more dependencies version",
      "releaseType": "patch",
      "color": "#8732bc"
    },
    {
      "name": "devops",
      "changelogTitle": "ğŸš¢ DevOps",
      "description": "Improve release pipelines, deployment, and tooling",
      "releaseType": "patch",
      "color": "#2f99a3"
    },
    {
      "name": "documentation",
      "changelogTitle": "ğŸ“ Documentation",
      "description": "Changes only affect the documentation",
      "releaseType": "patch",
      "color": "#cfd3d7"
    },
    {
      "name": "skip-release",
      "description": "Preserve the current version when merged",
      "releaseType": "skip",
      "color": "#bf5416"
    }
  ]
}
